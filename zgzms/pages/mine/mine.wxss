/* pages/mine/mine.wxss */
page{
  background: #f5f5f5;
}
.curriculum_list {
    position: relative;
    background: #fff;
    border-left: none;
    border-right: none;
    padding-left: 24rpx;
    margin-bottom: 20rpx;
}
.curriculum_list_info {
    padding-right: 24rpx;
    display: block;
}
.curriculum_list_info .class_names {
    display: inline-block;
    text-align: left;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    width: 100%;
    font-size: 30rpx;
    padding: 30rpx 30rpx 0 0;
}
.curriculum_list .keep {
    color: #999;
    padding-right: 24rpx;
}
.curriculum_list_info .start_time {
    margin: 0 0 24rpx 0px;
    color: #999;
    font-size: 24rpx;
}
.live-stuta{
  display: inline-block;
  color: #3eccb3;
  border: 1px solid #3eccb3;
  font-size: 20rpx;
  height: 30rpx;
  line-height: 30rpx;
  padding: 0 10rpx;
  border-radius: 10rpx;
}
.live-img{
  width: 20rpx;
  vertical-align: middle;
  margin-right:10rpx;
}
.live-stuta-4{
  display: inline-block;
  background: #fff;
  color: #3eccb3;
  font-size: 24rpx;
  height: 40rpx;
  line-height: 40rpx;
  padding: 0 10rpx;
  border-radius: 10rpx;
  border: 1px solid #3eccb3;
}
.container{
  padding: 0 53rpx;
}
.classtype{
  width: 534rpx;
  height: 186rpx;
  background: #fff;
  padding: 70rpx 45rpx 0;
  margin:64rpx auto 0;
  border-radius: 10rpx;
  font-size: 30rpx;
  text-align: center;
}
.classtype view{
  margin-bottom: 48rpx;
  font-size: 30rpx;
  color: #333;
}
.classtype .go-class{
  width: 100%;
  height: 78rpx;
  background: #ff6b49;
  line-height: 78rpx;
  color: #fff;
  font-size: 34rpx;
  border-radius: 5rpx;
}
.header{
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  color: #333;
  font-size: 24rpx;
}
.start_time{
  font-size: 24rpx;
  color: #999;
  padding:20rpx 0 30rpx;

}
/*输入手机号  */
.fix_mask{
  position: fixed;
  z-index: 999;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  width: 100%;
  height: 100%;
  background: rgba(0,0,0,0.5);
}

.father {
  position: absolute;
  left: 0;
  width: 580rpx;
  height: 660rpx;
  background: #fff3e8;
  z-index: 9999;
}
.coup-tit{
  font-size: 36rpx;
  color: #ff830e;
  margin: 40rpx 0 25rpx;
  text-align: center;
}
.coup-con{
  padding: 0 30rpx 30rpx;
}
.coup-con view{
  font-size: 24rpx;
  line-height: 40rpx;
  color: #333;
}
#mobile {
  height: 80rpx;
  line-height: 80rpx;
  width: 70%;
  margin: 0 auto;
  padding-left: 20rpx;
  background: #fff;
  font-size: 30rpx;
}
.resets_line {
  height: 80rpx;
  line-height: 80rpx;
  border-bottom: none;
  padding: 0;
  position: relative;
  background: #fff;
}
.resets_line input {
  border: none;
  padding-left: 20rpx;
  display: inline-block;
  width:300rpx;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 30rpx;

}
.resets_line button {
    position: absolute;
    right:30rpx;
    top:15rpx;
    height: 70rpx;
    width: 160rpx;
    border: 1px solid #ff6b49;  
    background: #fff;
    color: #ff6b49;
    border-radius: 10rpx;
    float: right;
    font-size: 28rpx;
    line-height: 70rpx;
    padding: 0;
    z-index: 999;
}
.resets_line .yzm-btn{
  background: #ccc;    
  color: #fff;
}
.coup-content{
  position: fixed;
  width: 580rpx;
  height: 620rpx;
  left: 50%;
  top: 50%;
  margin-left: -290rpx;
  margin-top: -310rpx;
  z-index: 9999;
}
.buttons{
  position: absolute;
  z-index: 10001;
  width: 280rpx;
  height: 80rpx;
  line-height: 80rpx;
  border-radius: 50rpx;
  bottom: 40rpx;
  left: 0;
  margin-left: 140rpx;
  padding: 0;
  background: #ff830e;
  color: #fff;
}
.buttons image{
  width: 100%;
  height: 100%;
}
