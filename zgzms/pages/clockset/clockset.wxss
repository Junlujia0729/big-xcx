


page{
  background: #f3f4f5;
}

.container-card{
  background: #fff;
  padding-left: 30rpx;
}
.clock{
  width: 26rpx;
  height: 26rpx;
  display: inline-block;
  padding-right: 15rpx;
  position: relative;
  bottom: -3rpx;
}
.card-item{
  padding: 30rpx 30rpx 30rpx 0;
  border-bottom: 1px solid #e5e5e5;
  font-size: 24rpx;
  color: #999;
  position: relative;
}
.card-item:last-child{
  border-bottom: none;
}
.item-100{
  height: 60rpx;
  line-height: 60rpx;
  font-size: 30rpx;
  color: #333;
}
.time{
  float: right;
  color: #999;
}
.set-tiem{
  width: 15rpx;
  height: 20rpx;
  margin-top: 15rpx;
  display: inline-block;
  margin-left: 20rpx;
}
.body-view{
  float: right;
}
.info{
  color: #333;
  font-size: 28rpx;
}
.mobile{
  float: right;
  font-size: 28rpx;
}