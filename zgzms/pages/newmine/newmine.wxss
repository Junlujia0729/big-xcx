page{
  height: 100%;
}
.container {
  flex-direction: column;
  font-size: 32rpx;
  background: #fff;
  padding: 20rpx;
}
.tips{
  font-size: 32rpx;
  line-height: 50rpx;
  padding-top:20rpx;
  color: #3eccb3;
  text-align: center;

}
.dia-title{
  font-size: 36rpx;
  line-height: 36rpx;
  color: #3eccb3;
  margin:30rpx 0;
  text-align: center;
  font-weight: 500;
}
.type{
  font-size: 30rpx;
  line-height: 30rpx;
  padding: 10rpx;
  background: #f2f3f4;
  color: #333;
  display: inline-block;
  border-radius: 8rpx;
  margin-bottom: 20rpx;
  margin-top: 30rpx;
}
.dia-tips{
  font-size: 24rpx;
  line-height: 24rpx;
  color: #666;
  padding-bottom: 30rpx;
  text-align: left;
}
.copy{
  width: 440rpx;
  height: 80rpx;
  background: #3eccb3;
  line-height: 80rpx;
  font-size: 30rpx;
  color: #fff;
  text-align: center;
  margin:80rpx auto 30rpx;
}


