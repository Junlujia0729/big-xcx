
page{
  background: #f5f5f5;
}
.M_logistic_info {
    background: #fff;
}
.M_logistic_info .M_logistic_div {
    position: relative;
    padding: 0 24rpx 0 24rpx;
    line-height: 100rpx;
    height: 100rpx;
    border-bottom: #666;
    overflow: hidden;
}
.M_logistic_info .logistic_address {
    color: #666;
    font-size: 28rpx;
    float: left;
}
.edit-address{
  position: absolute;
  right: 24rpx;
  top: 0;
  margin: auto;
  color: #666;
  font-size: 24rpx;
}
.edit-address image{
    width: 12rpx;
    height: 20rpx;
    display: inline-block;
    margin-top: 40rpx;
}
.logistic_address image{
  display: inline-block;
  width: 30rpx;
  height: 40rpx;
  vertical-align: middle;
  margin-right: 10rpx;
}
.line{
    border-bottom: 1px #ddd solid;
    margin-left: 24rpx;
}
.M_logistic_info .user_info {
    background: #fff;
    padding: 24rpx 24rpx 0 24rpx;
    overflow: hidden;
}
.M_logistic_info .user_info .dresslist{
  font-size: 30rpx;
  line-height: 36rpx;
  height: 36rpx;
  color: #666;
  float: left;
}
.M_logistic_info .user_info .phone{
  font-size: 30rpx;
  line-height: 36rpx;
  color: #666;
  float: right;
}
.address{
  color: #999999;
  padding: 12rpx 0 12rpx 24rpx;
  font-size: 24rpx;
}
.box{
  height: 90rpx;
  background: #f5f5f5;
  line-height: 90rpx;
  padding-left: 30rpx;
  color: #999;
  font-size: 30rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.box text{
  font-size: 20rpx;
  color: #999;
  display: inline-block;
  padding-left: 10rpx;
}
.M_logistic_info .class_info {
    padding: 24rpx;
    background: #fff;
    text-align: left;
}
.M_logistic_info .class-right {
    margin-left: 260rpx;
    background: #fff;
}
.class_info image{
  width: 248rpx;
  height: 158rpx;
  display: block;
  border: 0;
  float: left;
}
.class_info .classname {
    text-align: left;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    word-break: break-all;
    overflow: hidden;
    font-size: 30rpx;
    height:80rpx;
    line-height: 40rpx;
    color: #666;
    width: 560rpx;
    float: left;
}
.class_info .date {
    color: #999;
    font-size: 28rpx;
    line-height: 30rpx;
    padding: 10rpx 0;
    display: block;
}
.class_info .price {
    color: #fe6c25;
    font-size: 36rpx;
    height: 80rpx;
    margin-left: 600rpx;
    text-align: right;
}
.class_info .price text{
  font-size: 28rpx;
}
.M_logistic_info .period {
    position: relative;
    padding: 0 24rpx;
    background: #fff;
    height: 100rpx;
    line-height: 100rpx;
    font-size: 32rpx;
    color: #666;
}
.period .icon{
    width: 12rpx;
    height: 20rpx;
    display: inline-block;
    margin-top: 40rpx;
    position: absolute;
    right: 24rpx;
    top: 0;
}
.coupon{
  display: inline-block;
  position: absolute;
  right: 30rpx;
  top: 0;
  font-size: 24rpx;
}
.M_logistic_info .logistic_address {
    color: #333;
    font-size: 30rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}
.M_logistic_info .logistic_address text {
    background: #fc645f;
    color: #fff;
    padding: 6rpx;
    font-size: 24rpx;
    margin-left: 10rpx;
}
.examlevel{
  position: absolute;
  right: 48rpx;
  top: 30%;
  line-height: 40rpx;
  color: #666;
  font-size: 24rpx;
}
.footer{
  position: fixed;
  bottom:0;
  left:0;
  background: #fff;
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  padding-left: 24rpx;
}
.total{
  font-size: 34rpx;
}
.footer button {
position:absolute;
right:40rpx;
bottom:10rpx;
box-sizing:border-box;
font-size:32rpx;
width: 320rpx;
text-align:center;
text-decoration:none;
-webkit-tap-highlight-color:transparent;
overflow:hidden;
color:#fff;
background-color:#ff6b49;
height: 76rpx;
border-radius: 5rpx;
} 
.footer .price{
  color:#fe6c25;
}
.addressshow{
  padding:12rpx 24rpx 12rpx 24rpx;

}

.illustration{
  width: 100%;
  height: 22rpx;
  display: block;
}
.userinfo{
  background: #fff;
  padding-left: 30rpx;
  overflow: hidden;
}
.userinfo view{
  height: 90rpx;
  line-height: 90rpx;
  font-size: 30rpx;
  color: #333;
  border-bottom: 1px solid #ddd;
}
.userinfo view:last-child{
  border-bottom: none;
}
.userinfo view text{
  display: inline-block;
  width: 150rpx;
}
.userinfo view input{
  display: inline-block;
  width: 540rpx;
  vertical-align: middle;
  text-align: right;
}


/*ios支付弹窗  */
.mask{
  width: 100%;
  height: 100%;
  background: rgba(0,0,0,.4);
  position: fixed;
  top:0;
  left: 0;
  z-index: 999;
}
.mask-content{
  width: 560rpx;
  height: 320rpx;
  position: absolute;
  left: 50%;
  top:50%;
  margin-left: -280rpx;
  margin-top: -160rpx;
  background: #fff;
  font-size: 34rpx;
  line-height: 34rpx;
  border-radius: 10rpx;
  text-align: center;
  padding-top: 60rpx;
  overflow: hidden;
}
.clickok{
  border-top:solid 1px #ddd;
}
.mask-tip{padding-bottom: 36rpx;font-size:36rpx;}
.mask-tip2{padding-bottom: 36rpx;font-size:36rpx;}
.mask-tip2-r{font-weight: bolder;color:#030000;}
.mask-content button {
  box-sizing:border-box;
  font-size:34rpx;
  width: 320rpx;
  text-align:center;
  text-decoration:none;
  -webkit-tap-highlight-color:transparent;
  overflow:hidden;
  color:#3eccb3;
  background-color:#fff;
  height: 100rpx;
  border-radius: 5rpx;
  border:none;
  line-height: 100rpx;
} 
.mask-content button::after{
  border:none;
}