
page{
  background: #fff;
  font-size: 28rpx;
}

.examheader{padding:74rpx 0 68rpx;text-align: center;}
.banner{width: 100%;}
.banner image{width: 100%;display: block;}
.examheader h2{font-size: 45rpx;color: #333;font-weight: bold;padding-bottom: 18rpx;}
.examheader .fromtime{font-size: 30rpx;color: #666;line-height: 32rpx;padding-bottom: 54rpx;}
.examinfo{font-size: 26rpx;color: #333;line-height: 28rpx;}
.examinfo text{padding-right: 25rpx;color: #333;}
.people_num{background: #f5f5f5;height: 84rpx;line-height: 84rpx;font-size: 30rpx;color: #666;text-align: center;}
.people_num text{color: #ff7545;font-weight: bold;}

.container navigator{height: 50rpx;padding:20rpx 30rpx;overflow: hidden;border-bottom: 1px solid #eee;font-size:28rpx;color: #999;}
.container navigator .info-left{float: left;width: 80%;line-height: 50rpx;white-space: nowrap; overflow: hidden; text-overflow: ellipsis;} 
.container navigator .info-right{float: right;padding:0 20rpx;background:#3eccb3;color: #fff;font-size: 24rpx;border-radius: 15px;line-height: 50rpx;}
.container navigator text{color: #3eccb3;padding-right: 30rpx;}
.welfare{padding-bottom: 50rpx;}
.welfare image{width: 100%;}
.welfare_tit image{width: 87rpx;height: 59rpx;margin-left: -10rpx;}
.we .welfare:last-child{padding-bottom: 120rpx;}
.welfare .welfare_tit{line-height: 59rpx;font-size: 36rpx;color: #333;margin-bottom: 54rpx;text-align: center;height: 59rpx;}
.welfare .welfare_tit text{background: #3eccb3;border-radius: 0 15px 15px 0;line-height:59rpx;display: inline-block;height: 59rpx;vertical-align: middle;padding-right: 28rpx;color: #fff;position: relative;top:-20rpx;}
.welfare .wechat{font-size: 28rpx;color: #666;padding-left: 30rpx;line-height: 45rpx;}
.welfare view{font-size: 28rpx;color: #666;padding:0 30rpx;line-height: 45rpx;}

.w1 view{color: #ff6b49;}
.w1 .time{background: #ff6b49;color: #fff;padding: 0 5rpx;border-radius: 5rpx;}
.w1.gray view{color: #ccc;}
.w1.gray .time{background: #ccc;}
.w1 .colon{padding: 0 5rpx;font-weight: bold;}
.bottom_fixed_buttons view button {margin: 0;}
.bottom_fixed_buttons view.w2 .timeOut{background: #ddd;}
.content{
  padding: 12rpx 60rpx;
  overflow: hidden;
  padding-bottom: 120rpx;
}
.content view{
  font-size: 26rpx !important;
  line-height: 40rpx;
  color: #666;
}
.content .class_tips_52_tit ,.content .class_problem_52_tit{
  text-align: center;
  padding: 20rpx 0;
  font-size: 30rpx !important;
}
.content .class_tips_52_tit .wxParse-inline,.content .class_problem_52_tit .wxParse-inline{
  text-align: center;
  padding: 20rpx 0;
  font-size: 30rpx !important;
  display: inline-block;
  color: #333;
}

.content .class_tips_52_tit::before,.content .class_problem_52_tit::before {
  content: "";
  display: inline-block;
  width: 10rpx;
  height: 10rpx;
  background: #3eccb3;
  position: relative;
  top:-5rpx;
  margin-right: 20rpx;
}
.class_tips_52_tit::after,.class_problem_52_tit::after{
  content: "";
  display: inline-block;
  width: 10rpx;
  height: 10rpx;
  background: #3eccb3;
  position: relative;
  top:-5rpx;
  margin-left: 20rpx;
}