
.container{
  width: 100%;
  height: 300rpx;
  background: linear-gradient(to bottom, #33edb4 0%,#33cce5 100%);
}
.countent{
  width: 100%;
  position: fixed;
  bottom:180rpx;
  left: 0;
}
.countdown{
  font-size: 90rpx;
  color: #fff;
  margin-bottom: 100rpx;
  text-align: center;
}
slider{
  width: 630rpx;
  height: 10rpx;
  margin: 0 auto;
}
.countent .wx-slider-handle{
  width:30rpx;
  height:30rpx;
  top:8px;
}
.start{
  width: 140rpx;
  height: 140rpx;
  margin: 222rpx auto 70rpx;
  display: block;
}
.again{
  width: 180rpx;
  height: 60rpx;
  border:1px solid #fff;
  text-align: center;
  color: #fff;
  line-height: 60rpx;
  font-size: 30rpx;
  margin: 0 auto;
  border-radius: 30rpx;
}
.tips{
  color:#666;
  font-size: 28rpx;
  padding-top: 20rpx;
  width: 80%;
  margin: 0 auto;
}