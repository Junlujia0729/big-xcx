
page{
  background: #f5f5f5;
}
.M_logistic_info {
    padding-bottom: 90rpx;
}
.M_logistic_info .M_logistic_div {
    position: relative;
    padding: 0 24rpx 0 24rpx;
    line-height: 100rpx;
    height: 100rpx;
    border-bottom: #666;
    overflow: hidden;
}
.M_logistic_info .logistic_address {
    color: #666;
    font-size: 28rpx;
    float: left;
}
.edit-address{
  position: absolute;
  right: 24rpx;
  top: 0;
  margin: auto;
  color: #666;
  font-size: 24rpx;
}



.M_logistic_info .user_info {
    background: #fff;
    overflow: hidden;
    margin-bottom: 20rpx;
}
.M_logistic_info .user_info .dresslist{
  font-size: 30rpx;
  line-height: 36rpx;
  height: 36rpx;
  color: #666;
  float: left;
}
.M_logistic_info .user_info .phone{
  font-size: 30rpx;
  line-height: 36rpx;
  color: #666;
  float: right;
}
.addressinfo{
  background: #fff;
  color: #999999;
  padding: 24rpx;
  font-size: 24rpx;
}
.address{
  color: #999999;
  font-size: 24rpx;
}
.box{
  width: 100%;
  height: 20rpx;
  background: #f5f5f5;
  border-left: none;
  border-right: none;
}

.footer{
  position: fixed;
  bottom:0;
  left:0;
  background: #fc645f;
  width: 100%;
  height: 86rpx;
  line-height: 86rpx;
  padding-left: 24rpx;
  text-align: center;
  color: #fff;
  font-size: 32rpx;
}



