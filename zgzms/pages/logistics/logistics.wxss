
page{
  background: #f5f5f5;
}
.M_logistic_info {
    background: #fff;
}
.M_logistic_info .M_logistic_div {
    position: relative;
    padding: 14rpx 24rpx 0 24rpx;
    min-height: 100rpx;
    border-bottom: #666;
    overflow: hidden;
}
.M_logistic_info .logistic_address {
    color: #666;
    font-size: 28rpx;
    float: left;
    
}
.edit-address{
  float:right;
  margin: auto;
  color: #666;
  font-size: 24rpx;
}
.edit-address image{
    width: 12rpx;
    height: 20rpx;
    display: inline-block;
    margin-top: 40rpx;
}
.logistic_address image{
  display: inline-block;
  width: 30rpx;
  height: 40rpx;
  vertical-align: middle;
  margin-right: 10rpx;
}
.line{
    border-bottom: 1px #ddd solid;
    margin-left: 24rpx;
}
.M_logistic_info .user_info {
    background: #fff;
    padding: 24rpx 24rpx 0 24rpx;
    overflow: hidden;
}
.M_logistic_info .user_info .dresslist{
  font-size: 26rpx;
  line-height: 36rpx;
  height: 36rpx;
  color: #666;
  float: left;
}
.M_logistic_info .user_info .phone{
  font-size: 30rpx;
  line-height: 36rpx;
  color: #666;
  float: right;
}
.address{
  color: #999999;
  padding: 12rpx 0 12rpx 24rpx;
  font-size: 24rpx;
}
.box{
  width: 100%;
  border: 1px solid #ddd;
  height: 20rpx;
  background: #f5f5f5;
  border-left: none;
  border-right: none;
}
.M_logistic_info .class_info {
    background: #fff;
}
.M_logistic_info .class-right {
    margin-left: 260rpx;
    background: #fff;
}
.class_info image{
  width: 248rpx;
  height: 158rpx;
  display: block;
  border: 0;
  float: left;
}
.class_info .classname {
    text-align: left;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    word-break: break-all;
    overflow: hidden;
    font-size: 30rpx;
    height:80rpx;
    line-height: 40rpx;
    color: #666;
}
.class_info .date {
    color: #999;
    font-size: 28rpx;
    line-height: 30rpx;
    padding: 10rpx 0;
    display: block;
}
.class_info .price {
    color: #fe6c25;
    font-size: 36rpx;
    line-height: 30rpx;
    margin-top: 20rpx;

}
.class_info .price text{
  font-size: 28rpx;
}
.M_logistic_info .period {
    position: relative;
    padding: 0 24rpx;
    background: #fff;
    height: 100rpx;
    line-height: 100rpx;
    font-size: 32rpx;
    color: #666;
}
.period .icon{
    width: 12rpx;
    height: 20rpx;
    display: inline-block;
    margin-top: 40rpx;
    position: absolute;
    right: 24rpx;
    top: 0;
}
.coupon{
  display: inline-block;
  position: absolute;
  right: 48rpx;
  top: 0;
  font-size: 24rpx;
}
.M_logistic_info .logistic_address {
    color: #666;
    font-size: 30rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}
.M_logistic_info .logistic_address text {
    background: #fc645f;
    color: #fff;
    padding: 6rpx;
    font-size: 24rpx;
    margin-left: 10rpx;
}
.examlevel{
  position: absolute;
  right: 48rpx;
  top: 30%;
  line-height: 40rpx;
  color: #666;
  font-size: 24rpx;
}
.footer{
  position: fixed;
  bottom:0;
  left:0;
  background: #fff;
  width: 100%;
  height: 86rpx;
  line-height: 86rpx;
  padding-left: 24rpx;
}
button {
position:absolute;
right:0;
bottom:0;
box-sizing:border-box;
font-size:34rpx;
width: 250rpx;
text-align:center;
text-decoration:none;
-webkit-tap-highlight-color:transparent;
overflow:hidden;
color:#fff;
background-color:#fc645f;
border-radius:0;
}
.footer .price{
  color:#fe6c25;
}
.addressshow{
  padding:12rpx 24rpx 12rpx 24rpx;

}
.left{
  width: 120rpx;
  float: left;
  height: 100%;
}
.right{
  margin-left: 120rpx;
}
.state{
  font-size: 24rpx;
  color: #999;
  padding: 28rpx;
  border-bottom: 1px solid #e5e5e5;
}
.state .state-con{
  line-height: 40rpx;
}
.state-icon{
  text-align: center;
  height: 100%;
}
.state-icon .state-line{
  display: block;
  height: 140rpx;
  width: 1px;
  background:#999;
  margin: 0 auto;
}
.state-icon .round{
  display: block;
  width: 14rpx;
  height: 14rpx;
  background: #e5e5ee;
  margin:0 auto;
  border-radius: 50%;
}
.state-icon .state-line{
  display: block;
  height: 140rpx;
  width: 1px;
  background:#e5e5ee;
  margin: 0 auto;
}
.state-icon .round-noe{
  display: block;
  width: 23rpx;
  height: 23rpx;
  background: #3eccb3;
  margin:0 auto;
  border-radius: 50%;
  margin-top: 20rpx;
}
.dresslist{

}
