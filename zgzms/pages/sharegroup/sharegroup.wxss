.mask-tip2{padding-bottom: 28rpx;font-size:28rpx;text-align: left;}
.mask-tip2-r{font-weight: bolder;color:#030000;}
.mask-content button {
  box-sizing:border-box;
  font-size:34rpx;
  width: 320rpx;
  text-align:center;
  text-decoration:none;
  -webkit-tap-highlight-color:transparent;
  overflow:hidden;
  color:#3eccb3;
  background-color:#fff;
  height: 100rpx;
  border-radius: 5rpx;
  border:none;
  line-height: 100rpx;
} 
.mask-content button::after{
