.mask{
  width: 100%;
  height: 100%;
  background: rgba(0,0,0,.4);
  position: fixed;
  top:0;
  left: 0;
  z-index: 999;
}
.mask-content{
  width: 560rpx;
  height: 300rpx;
  position: absolute;
  left: 50%;
  top:50%;
  margin-left: -280rpx;
  margin-top: -160rpx;
  background: #fff;
  font-size: 34rpx;
  line-height: 34rpx;
  border-radius: 10rpx;
  text-align: center;
  padding-top: 60rpx;
  overflow: hidden;
  padding:40rpx 20rpx 20rpx;

}
.clickok{
  border-top:solid 1px #ddd;
}
