
page{
  background: #f5f5f5;
}
.container-card{
  position: relative;
  background: #f5f5f5;
}

.container-card .section{
  background: #fff;
  padding: 30rpx 30rpx;
  font-size: 30rpx;
  height: 160rpx;
  position: relative;
  border-bottom:0.5px solid #e5e5e5;
}
.container-card .section image{
  width: 130rpx;
  height: 160rpx;
  float: left;
}
.container-card .section-msg {
  padding: 10rpx 0;
}
.container-card .section-msg text{
  display: block;
  padding-bottom: 10rpx;
  font-size: 32rpx;
}
.container-card .section .section-msg{
  margin-left: 150rpx;
}
.container-card .section-msg .date{
  color: #999;
  font-size: 24rpx;
}
